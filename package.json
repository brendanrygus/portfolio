{
  "name": "brendanrygus.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@styled-system/theme-get": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "polished": "^3.5.1",
    "ramda": "^0.27.0",
    "react": "0.0.0-experimental-94c0244ba",
    "react-cache": "^2.0.0-alpha.1",
    "react-dom": "0.0.0-experimental-94c0244ba",
    "react-error-boundary": "^2.3.1",
    "react-helmet-async": "^1.0.6",
    "react-intersection-observer": "^8.27.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-snap": "^1.23.0",
    "styled-components": "^5.2.0-test.10",
    "styled-system": "^5.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:server": "cd server && webpack-dev-server",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --project-token pftg6v8z2i",
    "lint:workflows": "yamllint .github/workflows/*.yml"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "import/order": [
        "error",
        {
          "pathGroups": [
            {
              "pattern": "~/**",
              "group": "internal"
            }
          ],
          "groups": [
            "builtin",
            "external",
            "internal",
            "parent",
            [
              "index",
              "sibling"
            ]
          ],
          "newlines-between": "always",
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@graphql-cli/coverage": "2.1.0",
    "@graphql-cli/diff": "2.1.0",
    "@graphql-cli/similar": "2.1.0",
    "@graphql-cli/validate": "2.1.0",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-essentials": "6.0.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/node-logger": "6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "6.0.21",
    "@svgr/cli": "^5.4.0",
    "babel-loader": "^8.1.0",
    "chromatic": "^5.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql-cli": "4.0.0",
    "husky": "^4.3.0",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "react-is": "^16.13.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "yaml-lint": "^1.2.4"
  },
  "resolutions": {
    "react": "0.0.0-experimental-94c0244ba"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix && yarn lint:workflows",
      "pre-push": ""
    }
  }
}
